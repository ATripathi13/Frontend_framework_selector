{
  "name": "frontend-framework-selector",
  "displayName": "Frontend Framework Selector",
  "description": "A decision-support tool for choosing frontend frameworks with comprehensive comparisons",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:frontend-framework-selector.compareFrameworks"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "frontend-framework-selector.compareFrameworks",
        "title": "Frontend Framework Selector: Compare Frameworks"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  }
}